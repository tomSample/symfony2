<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
    <header>
        <nav class="navbar">
            <ul class="navbar-list">
                <li class="navbar-item"><a class="navbar-link" href="{{ path('app_home') }}">Accueil</a></li>
                <li class="navbar-item"><a class="navbar-link" href="{{ path('app_projects') }}">Projets</a></li>
                <li class="navbar-item"><a class="navbar-link" href="{{ path('app_about') }}">Parcours</a></li>
                {# <li class="navbar-item"><a class="navbar-link" href="{{ path('contact') }}">Contact</a></li> #}
            </ul>
        </nav>
    </header>

    <main>
        {% block body %}{% endblock %}
    </main>

    <footer>
        <p>&copy; 2024 Mon Portfolio</p>
    </footer>
</body>
</html>